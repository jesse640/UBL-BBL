{
    "name": "my-github-project",
    "version": "1.0.0",
    "description": "A Node.js project with CI setup",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "lint": "eslint .",
        "test": "jest --coverage",
        "build": "echo 'Build step here...'",
        "precommit": "npm run lint && npm test"
    },
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "eslint": "^8.57.1",
        "eslint-plugin-react": "^7.37.4",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "typescript-eslint": "^8.26.0"
    },
    "jest": {
        "collectCoverage": true,
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        }
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
