{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api-docs(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
